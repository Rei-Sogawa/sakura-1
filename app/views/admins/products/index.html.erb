<p id="notice"><%= notice %></p>

<h1>Admins Products</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admins_products.each do |admins_product| %>
      <tr>
        <td><%= link_to 'Show', admins_product %></td>
        <td><%= link_to 'Edit', edit_admins_product_path(admins_product) %></td>
        <td><%= link_to 'Destroy', admins_product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admins Product', new_admins_product_path %>
